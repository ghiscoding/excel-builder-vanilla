{
  "name": "excel-builder-vanilla",
  "version": "2.1.8",
  "description": "An easy way of building Excel files with javascript",
  "keywords": [
    "excel",
    "javascript",
    "xlsx",
    "spreadsheet"
  ],
  "author": "Stephen Liberty",
  "contributors": [
    {
      "name": "Ghislain B."
    }
  ],
  "homepage": "https://github.com/ghiscoding/excel-builder-vanilla",
  "bugs": {
    "url": "https://github.com/ghiscoding/excel-builder-vanilla/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghiscoding/excel-builder-vanilla.git",
    "directory": "packages/excel-builder-vanilla"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/excel-builder.umd.js",
  "module": "./dist/excel-builder.es.js",
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/excel-builder.umd.js",
      "require": "./dist/excel-builder.umd.js",
      "default": "./dist/excel-builder.es.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite build --watch",
    "dev:init": "pnpm clean && vite build",
    "build:watch": "vite build --watch"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2"
  }
}
